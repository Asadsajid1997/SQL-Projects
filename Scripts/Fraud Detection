DROP TABLE IF EXISTS buyerids_for_data_all;
CREATE TABLE buyerids_for_data_all LIFECYCLE 7 AS
select concat(venture, "_", buyer_id) buyer_id, count(distinct order_number) total_orders from daraz_cdm.dwd_drz_trd_core_df
where ds = MAX_PT('daraz_cdm.dwd_drz_trd_core_df')
-- and TO_CHAR(fulfillment_create_date,'yyyymmdd') >= '20230101'
-- and TO_CHAR(fulfillment_create_date,'yyyymmdd') < '20231101'
and delivery_date is not null
and return_complete_date is null
group by concat(venture, "_", buyer_id);

select count(distinct buyer_id) from buyerids_for_data_all where total_orders > 100;


DROP TABLE IF EXISTS rejection_orders;
CREATE TABLE rejection_orders LIFECYCLE 7 AS           
select month_, venture, regional_category1_name, 
logistic_partner, payment_method, buyer_id,
count(distinct order_number) as total_orders,
count(distinct(case when failed_delivery_reason = 'customer_refuses_delivery' then order_number end)) rejected_orders,
count(distinct(case when return_complete_date is not null then order_number end)) returned_orders,
ROUND(count(distinct(case when failed_delivery_reason = 'customer_refuses_delivery' then order_number end))
/count(distinct order_number),4) as rejection_rate
from (
select TO_CHAR(fulfillment_create_date,'yyyymm') as month_, a.fulfillment_create_date, a.venture, a.regional_category1_name, 
CASE   WHEN TOLOWER(fm_3pl_name) LIKE '%dex%' THEN 'DEX'
                                WHEN TOLOWER(fm_3pl_name) LIKE '%shop%' THEN 'DEX'
                                WHEN TOLOWER(fm_3pl_name) LIKE '%lmp%' THEN 'LMP'
                                ELSE '3PL' end as logistic_partner, case when a.payment_method = 'COD' then 'COD' else 'Prepaid' end as payment_method, a.buyer_id,
b.lvl1_delivery_failed_ts, b.third_failed_delivery_reason, a.order_number, a.return_complete_date,  
REPLACE(REPLACE(REPLACE(COALESCE(

            3pl_final_failed_delivery_reason

           ,3pl_third_failed_delivery_reason

           ,3pl_second_failed_delivery_reason

           ,3pl_first_failed_delivery_reason

       )

       ,',',' '),'\r',' '),'\n',' ') 

        as failed_delivery_reason
from  daraz_cdm.dwd_drz_trd_core_df a
inner join  drzops_cdm.dwd_drz_lgt_dlv_pkg_df b
on a.order_number = b.platform_order_number
WHERE a.ds = MAX_PT('daraz_cdm.dwd_drz_trd_core_df')
and b.ds = MAX_PT('drzops_cdm.dwd_drz_lgt_dlv_pkg_df')
and TO_CHAR(a.fulfillment_create_date,'yyyymmdd') >= '20230101'
and TO_CHAR(a.fulfillment_create_date,'yyyymmdd') < '20231101'
and concat(a.venture, "_", a.buyer_id) in (select buyer_id from buyerids_for_data where total_orders > 10)
) 
group by month_, venture, regional_category1_name, logistic_partner, payment_method, buyer_id
;


